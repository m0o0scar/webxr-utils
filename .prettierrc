{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 120,
  "singleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "^@types/(.*)$",
    "^@pages/(.*)$",
    "^@components/(.*)$",
    "^@hooks/(.*)$",
    "^@utils/(.*)$",
    "^\\./",
    "^\\.\\./"
  ],
  "importOrderSeparation": true
}
