{
  "name": "@nospoon/webxr-utils",
  "version": "0.1.0",
  "types": "build/mjs/main.d.ts",
  "module": "build/mjs/main.js",
  "main": "build/cjs/main.js",
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "vite --host",
    "dev:port": "adb reverse tcp:3000 tcp:3000",
    "serve": "vite preview",
    "build": "run-s build:esm build:cjs",
    "prebuild": "shx rm -rf build",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "prepublish": "yarn build",
    "prettier": "prettier --check 'src/**/*.{js,ts,tsx}'",
    "prettier-fix": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint --ext .js --ext .ts --ext .tsx ./src",
    "lint-fix": "eslint --ext .js --ext .ts --ext .tsx src --fix",
    "codecheck": "run-s prettier-fix lint-fix"
  },
  "dependencies": {
    "@react-three/drei": "^9.37.1",
    "@react-three/fiber": "^8.8.10",
    "@react-three/xr": "^5.0.4",
    "@tailwindcss/typography": "^0.5.7",
    "daisyui": "^2.33.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "recoil": "^0.7.6",
    "three": "^0.145.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "find-up": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-eslint": "^1.8.1",
    "yarn-upgrade-all": "^0.7.1"
  }
}
